<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:padding="0dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:id="@+id/container"
        android:layout_gravity="center"
        android:backgroundTint="@color/card_bg_color"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        app:cardCornerRadius="20dp"
        app:cardElevation="5dp"
        >


        <LinearLayout
            android:id="@+id/ubah_keterampilan"
            android:layout_width="300dp"
            android:layout_height="match_parent"
            android:layout_marginRight="75dp"
            android:background="@color/card_bg_color"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="6dp">

            <LinearLayout
                android:layout_width="190dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_gravity="center">

                <LinearLayout

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/mench"
                        android:layout_marginLeft="0dp"
                        android:text="Semester"
                        android:textColor="#000000"
                        android:textSize="20dp" />

                    <TextView
                        android:id="@+id/semester"
                        android:fontFamily="@font/mench"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:text="1"
                        android:textColor="#000000"
                        android:textSize="20dp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_gravity="center">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="match_parent"
                        android:src="@drawable/ket"
                        />
                    <TextView
                        android:id="@+id/idRekapPresensi"
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:focusable="false"
                        android:text="id"
                        android:visibility="gone"
                        android:layout_marginLeft="10dp"
                        android:textColor="@android:color/black"
                        android:textSize="15sp"/>
                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:focusable="false"
                        android:text="Hadir"
                        android:layout_marginLeft="10dp"
                        android:textColor="@android:color/black"
                        android:textSize="15sp"/>
                    <TextView
                        android:id="@+id/hadir"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:focusable="false"
                        android:textAlignment="center"
                        android:layout_marginLeft="30dp"
                        android:text="1"
                        android:textColor="@android:color/black"
                        android:textSize="15sp"
                        android:gravity="center_horizontal" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginLeft="10dp"
                    android:layout_gravity="center">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="match_parent"
                        android:src="@drawable/ket_izin"
                        />
                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:focusable="false"
                        android:text="Izin"
                        android:layout_marginLeft="10dp"
                        android:textColor="@android:color/black"
                        android:textSize="15sp"/>
                    <TextView
                        android:id="@+id/izin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:focusable="false"
                        android:textAlignment="center"
                        android:layout_marginLeft="30dp"
                        android:text="1"
                        android:textColor="@android:color/black"
                        android:textSize="15sp"
                        android:gravity="center_horizontal" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginLeft="10dp"
                    android:layout_gravity="center">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="match_parent"
                        android:src="@drawable/ket_sakit"
                        />
                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:focusable="false"
                        android:text="Sakit"
                        android:layout_marginLeft="10dp"
                        android:textColor="@android:color/black"
                        android:textSize="15sp"/>
                    <TextView
                        android:id="@+id/sakit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:focusable="false"
                        android:textAlignment="center"
                        android:layout_marginLeft="30dp"
                        android:text="1"
                        android:textColor="@android:color/black"
                        android:textSize="15sp"
                        android:gravity="center_horizontal" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginLeft="10dp"
                    android:layout_gravity="center">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="match_parent"
                        android:src="@drawable/ket_alpha"
                        />
                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="match_parent"
                        android:focusable="false"
                        android:text="Alpha"
                        android:layout_marginLeft="10dp"
                        android:textColor="@android:color/black"
                        android:textSize="15sp"/>
                    <TextView
                        android:id="@+id/kosong"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:focusable="false"
                        android:layout_marginLeft="30dp"
                        android:text="1"
                        android:textAlignment="center"
                        android:textColor="@android:color/black"
                        android:textSize="15sp"
                        android:gravity="center_horizontal" />
                </LinearLayout>

